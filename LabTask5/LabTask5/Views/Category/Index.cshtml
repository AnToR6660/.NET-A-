@model IEnumerable<LabTask5.DTOs.CategoryDTO>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>
    @foreach (var data in Model)
    {
    <tr>
        <td>@data.Id</td>
        <td>@data.CategoryName</td>
    </tr>
    }
</table>
