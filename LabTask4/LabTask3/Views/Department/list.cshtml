
@{
    ViewBag.Title = "list";
}

<h2>Department list</h2>

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>id</th>
    </tr>
    @foreach (var department in Model)
    {
        <tr>
            <td>@department.name</td>
            <td>@department.id</td>
            <td><a href="/Department/Edit/@department.id" class="btn btn-success">Edit</a></td>
            <td><a href="/Department/Details/@department.id" class="btn btn-success">Details</a></td>
        </tr>

    }

</table>

