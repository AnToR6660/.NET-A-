@model IEnumerable<ZeroHungerV2.DTOs.ApprovedDTO>

@{
    ViewBag.Title = "AssignedEmployee";
}

<h2>AssignedEmployee</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Restaurant
        </th>
        <th>
            FoodItem
        </th>
        <th>
            Quantity
        </th>
        <th>
            Location
        </th>
        <th>
            Assigned Employee
        </th>


    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Restaurant.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Food.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Food.quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Restaurant.location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
            @Html.ActionLink("Details", "Details", new { id = item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.id })
        </td>
    </tr>
}

</table>
